<%layout('./partials/usuallayout')%>
  <main class="text-center" id="vue-search">
    <div>
      <div>
        <h2>Order Detail</h2>
      </div>
    </div>
    <div v-for="element in orderData" style="text-align:left; margin-left: 30px;" class="row">
      <p style="margin-right:40px" >Order Id: {{element.Id}}</p>
      <p style="margin-right:40px">Total Amount : {{element.Total}}</p>
      <p style="margin-right:40px">Start Date : {{element.EffectiveDate}}</p>
    </div>
    <div class="container-fluid">
        <table class="table table-hover table-striped">
            <thead>
                <tr>
                    <th scope="col">Draft</th>
                    <th scope="col">Confirm</th>
                    <th scope="col">Activated</th>
                    <th scope="col">Delivering</th>
                    <th scope="col">End</th>
                </tr>
            </thead>
            <tbody>
              <tr>
                <td colspan="5">
                    <div class="progress progress-striped active">
                        <div class="progress-bar"  role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" :style="'width:'+progress+'%'">
                        </div>
                  </div>
                  <div class="row" style="margin-left: 900px; margin-top: 10px;">
                    <button class="btn btn-primary" style="margin-right:40px" type="button">Active</button>
                    <button class="btn btn-danger" style="margin-right:40px" type="button">Cancel</button>
                    <button class="btn btn-success" style="margin-right:40px" type="button">Confirm</button>
                  </div>
                </td>               
              </tr>
            </tbody>
        </table>
    </div>

  
  </main>
  <script src="/javascripts/components/searchControl.js"></script>
